{
    "pipeline":{
        "environment_variables":[
            {
                "name":"MINIO_SERVER_URL",
                "value":"http://192.168.23.96:8181"
            }
        ],
        "timer":{
            "only_on_changes":true,
            "spec":"0 15 * ? * *"
        },
        "lock_behavior":"unlockWhenFinished",
        "materials":[
            {
                "attributes":{
                    "branch":"master",
                    "url":"https://github.com/li-jiayu/demoWarGradle.git"
                },
                "type":"git"
            },
            {
                "attributes":{
                    "pipeline":"demoWarGradle",
                    "stage":"build",
                    "auto_update":true,
                    "ignore_for_scheduling":false,
                    "name":"dependency"
                },
                "type":"dependency"
            }
        ],
        "name":"demoWarGradle_master_test_repo",
        "stages":[
            {
                "jobs":[
                    {
                        "name":"build",
                        "elastic_profile_id":"java8-gradle6-pyupload",
                        "timeout":1500,
                        "tasks":[
                            {
                                "attributes":{
                                    "arguments":[
                                        "build"
                                    ],
                                    "command":"gradle"
                                },
                                "type":"exec"
                            },
                            {
                                "attributes":{
                                    "arguments":[
                                        "/home/go/uploadArtifacts.py"
                                    ],
                                    "command":"python"
                                },
                                "working_directory":"build/libs/",
                                "type":"exec"
                            }
                        ]
                    }
                ],
                "name":"build"
            }
        ]
    },
    "group":"test"
}
