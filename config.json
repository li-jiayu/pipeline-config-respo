{
    "format_version":1,
    "group":"test",
    "name":"demoWarGradle_master_test",
    "enable_pipeline_locking":false,
    "timer":{
        "only_on_changes":true,
        "spec":"0 15 * ? * *"
    },
    "environment_variables":[
        {
            "name":"MINIO_SERVER_URL",
            "value":"http://192.168.23.96:8181"
        }
    ],
    "materials":[
        {
            "attributes":{
                "branch":"master",
                "url":"https://github.com/li-jiayu/demoWarGradle.git"
            },
            "type":"git"
        },
        {
            "attributes":{
                "pipeline":"demoWarGradle",
                "stage":"build",
                "auto_update":true,
                "ignore_for_scheduling":false,
                "name":"dependency"
            },
            "type":"dependency"
        }
    ],
    "stages":[
        {
            "name":"build",
            "jobs":[
                {
                    "name":"build",
                    "timeout":180,
                    "elastic_profile_id":"java8-gradle6-pyupload",
                    "tasks":[
                        {
                            "type":"exec",
                            "run_if":"passed",
                            "on_cancel":null,
                            "command":"build",
                            "arguments":[
                                "-j3",
                                "docs",
                                "install"
                            ],
                            "working_directory":null
                        },
                        {
                            "type":"exec",
                            "run_if":"passed",
                            "on_cancel":null,
                            "command":"python",
                            "arguments":[
                                "/home/go/uploadArtifacts.py"
                            ],
                            "working_directory":"build/libs/"
                        }
                    ]
                }
            ]
        }
    ]
}
