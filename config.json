                            "command":"build",
                            "arguments":[
                                "-j3",
                                "docs",
                                "install"
                            ],
                            "working_directory":null
                        },
                        {
                            "type":"exec",
                            "run_if":"passed",
                            "on_cancel":null,
                            "command":"python",
                            "arguments":[
                                "/home/go/uploadArtifacts.py"
                            ],
                            "working_directory":"build/libs/"
                        }
                    ]
                }
            ]
        }
    ]
}
