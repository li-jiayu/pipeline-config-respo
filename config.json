{
    "format_version":1,
    "group":"test",
    "name":"demoWarGradle_master_test",
    "enable_pipeline_locking":false,
    "parameters":[
        {
            "name":"param",
            "value":"parameter"
        }
    ],
    "timer":{
        "only_on_changes":true,
        "spec":"0 15 * ? * *"
    },
    "environment_variables":[
        {
            "name":"MINIO_SERVER_URL",
            "value":"http://192.168.23.96:8181"
        }
    ],
    "materials":[
        {
            "url":"https://github.com/li-jiayu/demoWarGradle.git",
            "branch":"master",
            "auto_update":true,
            "name":"test1",
            "type":"git",
            "shallow_clone":true
        },
        {
            "pipeline":"demoWarGradle",
            "stage":"build",
            "name":"dependency",
            "type":"dependency",
            "ignore_for_scheduling":false
        }
    ],
    "stages":[
        {
            "name":"build",
            "jobs":[
                {
                    "name":"build",
                    "timeout":180,
                    "elastic_profile_id":"java8-gradle6-pyupload",
                    "tasks":[
                        {
                            "type":"exec",
                            "run_if":"passed",
